here# define two sets
A = {1, 2, 3, 4}
B = {3, 4, 5, 6}

# perform union and intersection
union_set = A | B
intersection_set = A & B

# display results
print("Set A:", A)
print("Set B:", B)
print("Union:", union_set)
print("Intersection:", intersection_set)

Set A: {1, 2, 3, 4}
Set B: {3, 4, 5, 6}
Union: {1, 2, 3, 4, 5, 6}
Intersection: {3, 4}
